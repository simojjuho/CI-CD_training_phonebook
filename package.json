{
  "name": "puhelinluettelo_backend",
  "version": "0.0.1",
  "description": "\"Phonebook spa.\"",
  "main": "./src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src/index.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --verbose --runInBand --forceExit",
    "build:ui": "rm -rf build && cd ./src/frontend && npm run build && cp -r build ../../",
    "deploy": "fly deploy",
    "deploy:full": "npm run build:ui && npm run deploy",
    "logs:prod": "fly logs",
    "lint": "eslint .",
    "start:test": "NODE_ENV=test node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simojjuho/FullStack-osa-3.git"
  },
  "keywords": [
    "puhelinluettelo",
    "backend",
    "harjoitus",
    "training"
  ],
  "author": "Juho Simojoki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/simojjuho/FullStack-osa-3/issues"
  },
  "homepage": "https://github.com/simojjuho/FullStack-osa-3#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.7.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "cypress": "^13.6.2",
    "eslint": "^8.28.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3"
  }
}
